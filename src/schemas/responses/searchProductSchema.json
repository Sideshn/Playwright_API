{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Search Product Response",
    "type": "object",
    "required": ["products"],
    "properties": {
        "products": {
            "type": "array",
            "items": {
                "type": "object",
                "required": ["id", "name"],
                "properties": {
                    "id": { "type": "number" },
                    "name": { "type": "string" }
                }
            }
        }
    }
}

